<Application x:Class="AirLane.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AirLane"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Tips.xaml" />
                <ResourceDictionary Source="AddAirportStyles.xaml"/>
                <ResourceDictionary Source="AddAircraftStyles.xaml"/>
                <ResourceDictionary Source="AddCompanyStyles.xaml"/>
                <ResourceDictionary Source="AddFlightStyles.xaml"/>
                <ResourceDictionary Source="AddRouteStyles.xaml"/>
                <ResourceDictionary Source="AllFlightADMStyles.xaml"/>
                <ResourceDictionary Source="AllFlightsStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
